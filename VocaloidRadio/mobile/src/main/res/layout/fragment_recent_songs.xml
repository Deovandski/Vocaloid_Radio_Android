<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.VocaloidRadio.RecentSongsFragment">

    <!-- TODO: Update blank fragment layout --><![CDATA[
        android:layout_width="match_parent"
        android:layout_height="match_parent">



    ]]>

    <WebView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/recentSongsWebView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/albumCover1"
        android:layout_marginTop="46dp"
        android:minHeight="@dimen/abc_text_size_display_3_material"
        android:minWidth="@dimen/abc_text_size_display_3_material"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/albumCover2"
        android:layout_below="@+id/albumCover1"
        android:layout_alignLeft="@+id/albumCover1"
        android:layout_alignStart="@+id/albumCover1"
        android:layout_marginTop="64dp"
        android:minWidth="@dimen/abc_text_size_display_3_material"
        android:minHeight="@dimen/abc_text_size_display_3_material" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="SongName"
        android:id="@+id/songName"
        android:layout_alignTop="@+id/albumCover1"
        android:layout_toRightOf="@+id/albumCover1"
        android:layout_toEndOf="@+id/albumCover1"
        android:allowUndo="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Singer Name"
        android:id="@+id/singerName"
        android:layout_below="@+id/songName"
        android:layout_alignLeft="@+id/songName"
        android:layout_alignStart="@+id/songName" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Album"
        android:id="@+id/album"
        android:layout_below="@+id/singerName"
        android:layout_alignLeft="@+id/singerName"
        android:layout_alignStart="@+id/singerName" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="SongName2"
        android:id="@+id/songName2"
        android:layout_alignTop="@+id/albumCover2"
        android:layout_alignLeft="@+id/album"
        android:layout_alignStart="@+id/album" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Singer Name2"
        android:id="@+id/singerName2"
        android:layout_below="@+id/songName2"
        android:layout_alignLeft="@+id/songName2"
        android:layout_alignStart="@+id/songName2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Album2"
        android:id="@+id/album2"
        android:layout_below="@+id/singerName2"
        android:layout_alignLeft="@+id/singerName2"
        android:layout_alignStart="@+id/singerName2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Recent Tracks"
        android:id="@+id/RecentTracksTitle"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/albumCover3"
        android:layout_below="@+id/album2"
        android:layout_alignLeft="@+id/albumCover2"
        android:layout_alignStart="@+id/albumCover2"
        android:layout_marginTop="74dp"
        android:minHeight="@dimen/abc_text_size_display_3_material"
        android:minWidth="@dimen/abc_text_size_display_3_material" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="SongName3"
        android:id="@+id/songName3"
        android:layout_alignTop="@+id/albumCover3"
        android:layout_toRightOf="@+id/albumCover3"
        android:layout_toEndOf="@+id/albumCover3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Singer Name3"
        android:id="@+id/singerName3"
        android:layout_below="@+id/songName3"
        android:layout_alignLeft="@+id/songName3"
        android:layout_alignStart="@+id/songName3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Album3"
        android:id="@+id/album3"
        android:layout_below="@+id/singerName3"
        android:layout_alignLeft="@+id/singerName3"
        android:layout_alignStart="@+id/singerName3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/albumCover4"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/albumCover3"
        android:layout_alignStart="@+id/albumCover3"
        android:layout_marginBottom="44dp"
        android:minHeight="@dimen/abc_text_size_display_3_material"
        android:minWidth="@dimen/abc_text_size_display_3_material" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="SongName4"
        android:id="@+id/songName4"
        android:layout_alignTop="@+id/albumCover4"
        android:layout_alignRight="@+id/singerName"
        android:layout_alignEnd="@+id/singerName" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Singer Name4"
        android:id="@+id/singerName4"
        android:layout_below="@+id/songName4"
        android:layout_alignLeft="@+id/songName4"
        android:layout_alignStart="@+id/songName4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Album4"
        android:id="@+id/album4"
        android:layout_below="@+id/singerName4"
        android:layout_alignLeft="@+id/songName4"
        android:layout_alignStart="@+id/songName4" />

</RelativeLayout>
